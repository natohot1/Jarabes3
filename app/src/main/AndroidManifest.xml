<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felix.jarabes">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Presentacion" />
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".Medicina"
            android:label="@string/title_activity_medicina"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_fullscreen"
            android:theme="@style/FullscreenTheme" />
        <activity android:name=".Main3Activity" />
        <activity android:name=".Colaboradores" />
        <activity
            android:name=".Dedicatoria"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_dedicatoria"
            android:theme="@style/FullscreenTheme"></activity>
    </application>

</manifest>